{
  "name": "ugrade-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.12.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.2.2",
    "formik": "^1.4.3",
    "github-markdown-css": "^3.0.1",
    "history": "^4.7.2",
    "moment": "^2.24.0",
    "node-sass-chokidar": "^1.3.4",
    "node-sass-tilde-importer": "^1.0.2",
    "react": "^16.7.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.7.0",
    "react-markdown": "^4.0.6",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.4",
    "react-transition-group": "^2.5.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typescript": "3.3.1",
    "yup": "^0.26.10"
  },
  "scripts": {
    "start": "npm-run-all build:css --parallel watch:css start:ts",
    "build": "npm-run-all build:css build:ts",
    "start:ts": "react-scripts start",
    "build:ts": "react-scripts build",
    "build:css": "node-sass-chokidar --quiet src/ -o src/ --importer=node_modules/node-sass-tilde-importer",
    "watch:css": "node-sass-chokidar --quiet src/ -o src/ --importer=node_modules/node-sass-tilde-importer --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "23.3.13",
    "@types/moment": "^2.13.0",
    "@types/node": "10.12.21",
    "@types/react": "^16.8.1",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-transition-group": "^2.0.15",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-thunk": "^2.1.0",
    "@types/yup": "^0.26.7",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0"
  }
}
