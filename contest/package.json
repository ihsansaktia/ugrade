{
  "name": "ugrade",
  "version": "1.0.0",
  "description": "Simple competitive programming contest platform using contestant PC as grader",
  "scripts": {
    "start": "node dist/main.js",
    "build": "npm-run-all build:ts build:gql",
    "watch:ts": "tsc -w",
    "watch:node": "nodemon dist/main.js",
    "build:ts": "tsc",
    "build:gql": "cp src/schema.gql dist/",
    "watch:gql": "nodemon --watch src/schema.gql --exec \"npm run build:gql\"",
    "watch": "npm-run-all build:ts build:gql --parallel watch:ts watch:node watch:gql",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jauhararifin/ugrade.git"
  },
  "author": "Jauhar Arifin",
  "bugs": {
    "url": "https://github.com/jauhararifin/ugrade/issues"
  },
  "homepage": "https://github.com/jauhararifin/ugrade#readme",
  "dependencies": {
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.7",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  }
}
